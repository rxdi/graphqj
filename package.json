{
  "name": "@rxdi/graphqj",
  "main": "./lib/main.js",
  "version": "0.7.50",
  "scripts": {
    "start": "gapi start --local --minify=false --inspect",
    "start-client": "parcel ./src/app/client/application/index.html",
    "test": "npx jest",
    "lint": "npx eslint . --ext .ts",
    "lint-fix": "npx eslint . --fix --ext .ts",
    "introspect": "gapi schema introspect --collect-documents --collect-types",
    "build": "gapi build",
    "build-client": "parcel build ./src/app/client/application/index.html",
    "patch": "npm version patch && npm publish --update-readme --access public"
  },
  "dependencies": {
    "@gapi/core": "^1.8.23",
    "@popeindustries/lit-html-server": "^1.3.0",
    "@gapi/voyager": "^1.8.23",
    "chokidar": "^3.0.2",
    "graphql": "^14.6.0",
    "clear-module": "^4.0.0",
    "esm": "^3.2.25",
    "fast-glob": "^3.0.4",
    "js-yaml": "^3.13.1"
  },
  "devDependencies": {
    "@babel/polyfill": "7.4.4",
    "@rxdi/graphql-client": "^0.7.16",
    "husky": "^4.2.3",
    "@rxdi/lit-html": "^0.7.16",
    "@rxdi/parcel-plugin-shebang": "^0.7.39",
    "@rxdi/router": "^0.7.16",
    "@types/hapi": "^18.0.2",
    "@types/jest": "25.1.2",
    "@types/js-yaml": "^3.12.1",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "jest": "25.1.0",
    "jest-cli": "25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "25.2.1",
    "@webcomponents/webcomponentsjs": "1.3.3",
    "core-js": "^2.6.5",
    "cssnano": "^4.1.10",
    "graphql-tag": "^2.10.1",
    "himalaya": "^1.1.0",
    "typescript": "^3.5.3"
  },
  "esm": {
    "cjs": {
      "cache": false
    }
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rxdi/graphqj"
  },
  "bin": {
    "gj": "./dist/main.js",
    "gg": "./dist/main.js",
    "graphqj": "./dist/main.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "types": "./lib/index.d.ts",
  "module": "./lib/index.js",
  "typings": "./lib/index.d.ts"
}
